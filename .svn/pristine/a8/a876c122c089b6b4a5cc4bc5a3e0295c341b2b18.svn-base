
<ion-header-bar align-title="center" class="bar-positive">
    <div class="buttons">
        <button class="button button-icon ion-ios-arrow-thin-left" ng-click="$ionicGoBack()"></button>
    </div>
    <h1 class="title">农机服务地图</h1>
</ion-header-bar>
<ion-content id="mapContainer" overflow-scroll="false">
    
</ion-content>

<div id="mapSideBox" class="map-side-menu">

    <div class="row">
        <div class="col">
            <a class="button button-full button-small button-positive" ng-click="onChecked('All')">
                全选
            </a>
        </div>
        <div class="col">
            <a class="button button-full button-small button-positive" ng-click="onChecked('None')">
                全不选
            </a>
        </div>
    </div>
    <div class="list">
        <ion-checkbox ng-repeat="item in ListSideMenu" ng-model="item.checked" ng-checked="item.checked" ng-change="onChecked(item)">{{item.Name}}</ion-checkbox>
    </div>
</div>

