<ion-modal-view>
    <ion-header-bar align-title="center" class="bar-positive">
        <div class="buttons" ng-click="onCloseDlgPriceSubCategory()">
            <a class="button button-clear">取消</a>
        </div>
        <h1 class="title">选择类别</h1>
    </ion-header-bar>
    <ion-content class="dg">
        <div class="row" ng-repeat="item in ListSubCategory" ng-click="onSelectSubCategory(item)">
            <div class="col">{{item.SName}}</div>
        </div>
    </ion-content>
</ion-modal-view>