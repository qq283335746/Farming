
<ion-header-bar align-title="center" class="bar-positive">
    <div class="buttons">
        <button class="button button-icon ion-ios-arrow-thin-left" ng-click="$ionicGoBack()"></button>
    </div>
    <h1 class="title">{{title}}</h1>
</ion-header-bar>
<ion-view>
    <ion-content overflow-scroll="false" class="dg-list">
        <ion-refresher pulling-text="正在刷新..." spinner="lines" pulling-icon="ion-arrow-down-c" on-refresh="fetch(true)"></ion-refresher>
        <ion-list>
            <ion-item ng-repeat="item in dataArr" href="#/tab/mineQuestionDetail/{{item.Id}}/{{ModelData.RoleType}}" class="item-icon-right">
                {{item.Title}}
                <i class="icon ion-ios-arrow-right f-dark" style="font-size:16px;"></i>
            </ion-item>
        </ion-list>
    </ion-content>
</ion-view>