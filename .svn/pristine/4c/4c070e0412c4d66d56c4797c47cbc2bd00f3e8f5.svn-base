
<ion-header-bar align-title="center" class="bar-positive">
    <div class="buttons">
        <button class="button button-icon ion-ios-arrow-thin-left" ng-click="$ionicGoBack()"></button>
    </div>
    <h1 class="title">个人资料</h1>
    <div class="buttons" ng-click="save()">
        <a class="button button-clear">保存</a>
    </div>
</ion-header-bar>
<ion-view id="personal">
    <ion-content has-bouncing="true" overflow-scroll="false">
        <ion-list>
            <div class="blank-h12"></div>
            <img src="{{msg.Photo}}" style="width: 120px;height: 120px;display: block;margin: auto;" ng-click="changeIcon()">
            <div class="blank-h12"></div>
            <ion-item class="mine-msg-item item">
                <span class="msg">真实姓名：</span>
                <input class="textfield ng-pristine ng-untouched" style="border: 0;" placeholder="请输入姓名" ng-model="msg.TrueName">
            </ion-item>
            <ion-item class="mine-msg-item item">
                <span class="msg">性别：</span>
                <select style="width: 120px;position: absolute;left: 54px;top: 13px;" ng-model="msg.sex">
                    <option>男</option>
                    <option>女</option>
                </select>
            </ion-item>
            <ion-item class="mine-msg-item item">
                <span class="msg">地区：</span>
                <input style="background: transparent;padding: 0;width: 280px;position: absolute;left: 55px;top: 8px;" type="text" placeholder="请选择地区" readonly="" ng-model="msg.Address" ng-click="showCitySelect()">
            </ion-item>
        </ion-list>
    </ion-content>

    <!--选择地区-->
    <div class="js-citySelestNode tab-nav tabs hide-on-keyboard-open"  style="padding:0 10px;width:100%;height:auto;background-color:#fff;"></div>
</ion-view>