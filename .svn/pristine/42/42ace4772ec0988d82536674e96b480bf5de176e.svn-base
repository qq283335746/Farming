}

// navigate page
.navigate-div {
  border-bottom: 1px solid lightgray;
  position: relative;
  height: 48px;
  background-color: white;
}
.navigate-img {
  position: absolute;
  width: 28px;
  height: 28px;
  left: 8px;
  top: 10px;
}
.navigate-title {
  line-height: 44px;
  margin: 0;
  padding-left: 52px;
  color: #3e3e3e;
}
.navigate-cross {
  position: absolute;
  right: 10px;
  top: 13px;
  color: lightgray;
}

// professor page
.post-input {
  width: 74%;
  left: 10px;
  position: absolute;
  height: 36px;
  top: 6px;
  background-color: transparent;
  border-bottom: 1px solid dimgray;
  font-size: 15px;
}
.post-send-btn {
  position: absolute;
  right: 2px;
  height: 30px;
  top: 9px;
}

// question page
.mine-msg-item {
  height: 52px;
  padding: 0px;
}
.mine-msg-item .msg {
  position: absolute;
  left: 10px;
  top: 15px;
}
.mine-msg-item .textfield {
  position: absolute;
  left: 133px;
  width: 50%;
  top: 14px;
  border-width: 0;
  background-color: white;
}
.mine-msg-item .cross {
  color: lightgray;
  position: absolute;
  right: 10px;
  top: 14px;
}
.mine-portarit {
  display: inline-block;
  width: 66px;
  height: 66px;
  vertical-align: top;
  position: relative;
  margin-bottom: 10px;
  margin-right: 5px;
}
.mine-portarit a {
  background: red;
  color: white;
  position: absolute;
  right: -5px;
  top: -5px;
  width: 20px;
  height: 20px;
  border-radius: 10px;
  text-align: center;
}
.mine-portarit img {
  width: 100%;
  height: 100%;
 }

