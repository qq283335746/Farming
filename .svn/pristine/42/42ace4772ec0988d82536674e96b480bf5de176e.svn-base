}
.blank-h12 {
  height: 12px;
}

.backgound-default {
  background-color: #f5f5f5;
}

.background-white {
  background-color: white;
}

// main page
.icon-tab {
  font-size: 24px !important;
  line-height: 33px !important;
}

.custome-div {
  padding: 4px;
  position: relative;
  background-color: white;
}
.custom-div-title {
  line-height: 28px;
}
.custom-div-icon {
  color: #33cd5f;
  font-size: 22px
}
.custom-div-text {
  font-weight: bold;
  color: gray;
  position: absolute;
  top: 4px;
  left: 30px;
}
.custom-div-more {
  position: absolute;right: 5px;top: 2px;
}

.div-text-point {
  position: relative;
  border-top: 1px solid lightgray;
}
.div-text-point-b {
  position: relative;
  border-bottom: 1px solid lightgray;
}
.div-text-point-p {
  position: absolute;
  background-color: #33cd5f;
  width: 8px;
  height: 8px;
  border-radius: 8px;
  top: 15px;
  left: 4px;
}
.div-text-point-text {
  margin-left: 20px;
  margin-bottom:0;
  height: 38px;
  line-height: 38px
}
.div-img-two {
  padding: 2px;
}
.div-img-title {
  text-align: center;
  font-size: 12px;
  border-left: 1px solid #e2e2e2;
  border-top: 1px solid #e2e2e2;
  line-height: 3px;
  height: 99px;
}
.div-img-title-img {
  display: block;
  width: 64px;
  height: 64px;
  margin-left: auto;
  margin-right: auto;
}

.div-circle-img {
  display: block;
  width: 100%;
  height: 100%;
  padding-left: 8px;
  padding-right: 8px;
}

// msgList page
.div-text-point-2 {
  position: relative;
  border-bottom: 1px solid lightgray;
}
.div-text-point-p-2 {
  position: absolute;
  background-color: #33cd5f;
  width: 8px;
  height: 8px;
  border-radius: 8px;
  top: 28%;
  left: 4px;
}
.div-text-point-text-2 {
  margin-left: 22px;
  line-height: 22px;
