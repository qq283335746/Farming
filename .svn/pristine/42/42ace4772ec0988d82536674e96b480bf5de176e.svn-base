@import 'iconfont/iconfont.css';

@import 'mixin';
@import 'register';
@import 'login';

.mt-20 {
  margin-top:dp(20) !important;
}
.mt-30 {
  margin-top:dp(30) !important;
}
.mt-40 {
  margin-top:dp(40) !important;
}
.rotate-180 {
  transform:rotate(180deg);
}
.dm-border {
  border-bottom:1px solid #e2e2e2;
}
.dm-border-top {
  border-top:1px solid #e2e2e2;
}
.dm-border-right {
  border-right:1px solid #e2e2e2 !important;
}
.pm-0 {
  margin:0 !important;
  padding:0 !important;
}
.p-0 {
  padding:0 !important;
}
.p-2 {
  padding: 2px;
}
.m-0 {
  margin:0 !important;
}
.no-border {
  border: none !important;
}
.b-0 {
  border: 0;
}

.no-item-wrap {
  padding-top: 40%;
  text-align: center;
  i {
    font-size: 60px;
  }
}

.dm-nowrap {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.dm-nowrap-1 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  word-break: break-all;
  white-space: normal !important;
}
.dm-nowrap-2 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  word-break: break-all;
  white-space: normal !important;
}
.dm-nowrap-3 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  word-break: break-all;
  white-space: normal !important;
}

.img-default {
  width: 80px;
  height: 80px;
}

// ÌáÊ¾¿ò
.dm-tips {
  z-index: 999;
  position: absolute;
  left:0;
  right:0;
  margin-left: auto;
  margin-right: auto;
  background-color: rgba(44,44,44,.7);
  color: #fff;
  text-align: center;
  top: 40%;
  max-width: 170px;
  padding: 12px;
  border-radius: 8px;
}
@keyframes tipsHide {
  0% {opacity: 1;}
  100% {opacity: 0;}
}

// normal
.blank-h44 {
